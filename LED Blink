//tinkercad : https://www.tinkercad.com/things/69hhtuC75Ac-1-led-blinking?sharecode=FRdRaXPRWBl-ctCEWEpGKDEkVTYQ15SE10qzoEvQktM
// Variable
// HIGH = ON
// LOW = OFF
// delay = timer

int RED = 5; //declare pin LED
// basic functions

void setup()
  
{
pinMode(RED, OUTPUT); 
}

void loop()
{
digitalWrite(RED, HIGH); 
  delay(500); //5 millisecond
digitalWrite(RED, LOW);
  delay (500);
}
